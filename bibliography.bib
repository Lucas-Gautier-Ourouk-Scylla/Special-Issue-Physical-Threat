@article{mohantyTopdownModulationAttention2013,
  title = {Top-down Modulation of Attention by Emotion},
  author = {Mohanty, Aprajita and Sussman, Tamara J.},
  year = {2013},
  month = apr,
  journal = {Frontiers in Human Neuroscience},
  volume = {7},
  publisher = {Frontiers},
