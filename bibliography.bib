}

@article{theeuwesAttentionalSetInteracts2004,
  title = {Attentional Set Interacts with Perceptual Load in Visual Search},
  author = {Theeuwes, Jan and Kramer, Arthur F. and Belopolsky, Artem V.},
  year = {2004},
  month = aug,
  journal = {Psychonomic Bulletin \& Review},
