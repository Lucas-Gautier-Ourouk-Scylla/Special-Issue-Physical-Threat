  title = {Anxiety and Cognitive Performance: {{Attentional}} Control Theory},
  shorttitle = {Anxiety and Cognitive Performance},
  author = {Eysenck, Michael W. and Derakshan, Nazanin and Santos, Rita and Calvo, Manuel G.},
  year = {2007},
  journal = {Emotion},
