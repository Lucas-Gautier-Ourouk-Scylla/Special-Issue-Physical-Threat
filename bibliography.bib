@article{sussmanItsAllAnticipation2016,
  title = {It's All in the Anticipation: {{How}} Perception of Threat Is Enhanced in Anxiety},
  shorttitle = {It's All in the Anticipation},
  author = {Sussman, Tamara J. and Szekely, Akos and Hajcak, Greg and Mohanty, Aprajita},
