@article{hutchinsonInvestigationInattentionalBlindness2023,
  title = {An Investigation of Inattentional Blindness Using Gaze and Frequency Tagging},
  author = {Hutchinson, Brendan T. and Wilkinson, Natalie and Robertson, Gemma and Budd, Alycia and Nicholls, Michael E. R. and Griffiths, Oren},
