@article{remingtonCanSeeClearly2014,
  title = {I Can See Clearly Now: The Effects of Age and Perceptual Load on Inattentional Blindness},
  shorttitle = {I Can See Clearly Now},
  author = {Remington, Anna and {Cartwright-Finch}, Ula and Lavie, Nilli},
