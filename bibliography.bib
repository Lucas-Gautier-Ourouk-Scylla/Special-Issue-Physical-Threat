@article{furlanettoValidityBeckDepression2005,
  title = {The Validity of the {{Beck Depression Inventory-Short Form}} as a Screening and Diagnostic Instrument for Moderate and Severe Depression in Medical Inpatients},
  author = {Furlanetto, Let{\'i}cia M. and Mendlowicz, Mauro V. and Romildo Bueno, J.},
  year = {2005},
