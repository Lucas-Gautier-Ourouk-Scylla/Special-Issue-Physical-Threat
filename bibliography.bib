}

@article{beaurenautThreatScreamParadigm2020,
  title = {The `{{Threat}} of {{Scream}}' Paradigm: A Tool for Studying Sustained Physiological and Subjective Anxiety},
  shorttitle = {The `{{Threat}} of {{Scream}}' Paradigm},
  author = {Beaurenaut, Morgan and Tokarski, Elliot and Dezecache, Guillaume and Gr{\`e}zes, Julie},
  year = {2020},
  month = jul,
  journal = {Scientific Reports},
  volume = {10},
  number = {1},
