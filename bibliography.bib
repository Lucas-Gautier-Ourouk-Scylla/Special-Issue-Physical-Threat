@article{wybleAttentionalBlinkProvides2009,
  title = {The Attentional Blink Provides Episodic Distinctiveness: {{Sparing}} at a Cost},
  shorttitle = {The Attentional Blink Provides Episodic Distinctiveness},
  author = {Wyble, Brad and Bowman, Howard and Nieuwenstein, Mark},
