@article{houstonControlStressLocus1972,
  title = {Control over Stress, Locus of Control, and Response to Stress},
  author = {Houston, B. Kent},
  year = {1972},
  journal = {Journal of Personality and Social Psychology},
  volume = {21},
  number = {2},
  pages = {249--255},
  publisher = {American Psychological Association},
  address = {US},
  issn = {1939-1315},
  doi = {10.1037/h0032328},
  abstract = {Tested R. S. Lazarus' proposal that the less control a person judges himself to have in a threatening situation, the more stressful it will be. A total of 66 male undergraduates was given the Digits Backward test of the WAIS under conditions of nonstress, avoidable shock, or unavoidable shock. Stress was measured by Ss' EKG, heart rate, and responses to Zuckerman's Affect Adjective Check List. It was found that Ss in the unavoidable shock group reported more anxiety but evidenced less physiological arousal than the avoidable shock group. Lazarus also asserts that general beliefs about helplessness increase the perception of threat. Contrary to expectation, external locus-of-control Ss (assumed to hold general feelings of helplessness) manifested less physiological arousal in stress than internal-control Ss, as measured by the Internal-External Control Scale. Results congruent with previous findings were obtained between locus of control and task performance. (19 ref.) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {Anxiety,Internal External Locus of Control,Physiological Arousal,Stress,Threat},
