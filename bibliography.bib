@article{basanovicDoesAnxietylinkedAttentional2017,
  title = {Does Anxiety-Linked Attentional Bias to Threatening Information Reflect Bias in the Setting of Attentional Goals, or Bias in the Execution of Attentional Goals?},
  author = {Basanovic, Julian and MacLeod, Colin},
  year = {2017},
