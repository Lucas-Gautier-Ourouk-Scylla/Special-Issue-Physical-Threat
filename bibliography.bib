@article{walkerBaselFaceDatabase2018,
  title = {The {{Basel Face Database}}: {{A}} Validated Set of Photographs Reflecting Systematic Differences in {{Big Two}} and {{Big Five}} Personality Dimensions},
  shorttitle = {The {{Basel Face Database}}},
  author = {Walker, Mirella and Sch{\"o}nborn, Sandro and Greifeneder, Rainer and Vetter, Thomas},
