  author = {Schmitz, Anja and Merikangas, Kathleen and Swendsen, Haruka and Cui, Lihong and Heaton, Leanne and Grillon, Christian},
  year = {2011},
  month = oct,
  journal = {Journal of Experimental Child Psychology},
