  author = {Hanslmayr, Simon and Backes, Heidelore and Straub, Sarah and Popov, Tzvetan and Langguth, Berthold and Hajak, G{\"o}ran and B{\"a}uml, Karl-Heinz T. and Landgrebe, Michael},
  year = {2013},
  journal = {Human Brain Mapping},
  volume = {34},
