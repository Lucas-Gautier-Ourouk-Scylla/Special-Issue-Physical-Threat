  keywords = {Adolescent,Adult,Affect,Aged,Aged 80 and over,Anxiety,Cross-Sectional Studies,Depression,Factor Analysis Statistical,Female,Humans,Male,Middle Aged,Reproducibility of Results,Severity of Illness Index,Stress Psychological,Surveys and Questionnaires}
}

@article{herbertEffectVaryingDanger2020,
