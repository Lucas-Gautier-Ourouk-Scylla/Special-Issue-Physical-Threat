@article{onieRelativeSensitivitySpatial2022,
  title = {On the Relative Sensitivity of Spatial and Nonspatial Measures of Attentional Bias: {{Emotion-induced}} Blindness, the Dot Probe, and Gradations in Ratings of Negative Pictures},
  shorttitle = {On the Relative Sensitivity of Spatial and Nonspatial Measures of Attentional Bias},
  author = {Onie, Sandersan and Most, Steven B.},
  year = {2022},
