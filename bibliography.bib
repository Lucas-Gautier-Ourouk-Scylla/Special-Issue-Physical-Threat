@article{kentridgeAttendedUnseenVisual2008,
  title = {Attended but Unseen: {{Visual}} Attention Is Not Sufficient for Visual Awareness},
  shorttitle = {Attended but Unseen},
  author = {Kentridge, R. W. and Nijboer, T. C. W. and Heywood, C. A.},
