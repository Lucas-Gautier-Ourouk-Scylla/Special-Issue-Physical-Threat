@article{mcmanusPsychologyPropertyPersons2023,
  title = {Psychology {{Is}} a {{Property}} of {{Persons}}, {{Not Averages}} or {{Distributions}}: {{Confronting}} the {{Group-to-Person Generalizability Problem}} in {{Experimental Psychology}}},
  shorttitle = {Psychology {{Is}} a {{Property}} of {{Persons}}, {{Not Averages}} or {{Distributions}}},
  author = {McManus, Ryan M. and Young, Liane and Sweetman, Joseph},
