@article{beaurenautActionCorepresentationThreat2021,
  title = {Action Co-Representation under Threat: {{A Social Simon}} Study},
  shorttitle = {Action Co-Representation under Threat},
  author = {Beaurenaut, Morgan and Dezecache, Guillaume and Gr{\`e}zes, Julie},
  year = {2021},
