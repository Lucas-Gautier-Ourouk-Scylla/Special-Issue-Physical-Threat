  langid = {english},
  keywords = {attentional set,inattentional blindness,semantic categorization,stimulus type},
  file = {C\:\\Users\\User.PSY-R-LUGAUTIE\\Zotero\\storage\\Q38XI4NL\\Guo et al. - 2016 - The Role of Stimulus Type and Semantic Category-Le.pdf;C\:\\Users\\User.PSY-R-LUGAUTIE\\Zotero\\storage\\WCV4MTEV\\jpr.html}
}

